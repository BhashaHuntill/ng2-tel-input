{
    "extends": "./tsconfig-esm2015.json",
    "compilerOptions": {
        "target": "es5",
        "outDir": "./tmp/esm5",
        "rootDir": "./tmp"
    },
    "angularCompilerOptions": {
        "annotateForClosureCompiler": true,
        "strictMetadataEmit": true,
        "skipTemplateCodegen": true,
        "flatModuleOutFile": "ng2-tel-input.js",
        "flatModuleId": "ng2-tel-input",
    }
}